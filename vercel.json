{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "BEARER_TOKEN": "@bearer_token"
  },
  "functions": {
    "app.py": {
      "maxDuration": 60
    }
  },
  "buildCommand": "pip install -r requirements.txt"
}
